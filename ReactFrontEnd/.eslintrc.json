{
  "env": { "browser": true, "es2021": true, "jest": true },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "prettier"
  ],
  "parserOptions": { "ecmaVersion": 12, "sourceType": "module" },
  "rules": {
    "react/prop-types": "off",
    "no-unused-vars": "warn"
  }
}